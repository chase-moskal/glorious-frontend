{
  "editor.detectIndentation": false,
  "editor.insertSpaces": true,
  "editor.tabSize": 2,
  "editor.renderWhitespace": "boundary",
  "editor.wordWrap": "on",
  "editor.wrappingIndent": "indent",
  "files.trimTrailingWhitespace": true,
  "files.eol": "\n",
  "files.encoding": "utf8",
  "files.exclude": {
    "build": true,
    "node_modules": true
  },
  "typescript.tsdk": "./node_modules/typescript/lib/"
}